<h1>Listing softwares</h1>

<table>
  <tr>
    <th>Workstation</th>
    <th>Wmi property</th>
    <th>Value</th>
  </tr>

<% @softwares.each do |software| %>
  <tr>
    <td><%=h software.workstation_id %></td>
    <td><%=h software.wmi_property_id %></td>
    <td><%=h software.value %></td>
    <td><%= link_to 'Show', software %></td>
    <td><%= link_to 'Edit', edit_software_path(software) %></td>
    <td><%= link_to 'Destroy', software, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New software', new_software_path %>