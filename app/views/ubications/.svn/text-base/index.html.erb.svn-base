<%= render :partial => "admin/section_top" %>
<div class="wmi_div">
<table class="wmi" cellpadding="0">
  <tr>
    <th class="wmi">Nombre</th>
    <th class="wmi">Descripción</th>
    <th class="wmi" colspan="3">Opciones</th>
  </tr>

<% @ubications.each do |ubication| %>
  <tr>
    <td class="wmi"><%=h ubication.name %></td>
    <td class="wmi"><%=h ubication.description %></td>
    <td class="wmi"><%= link_to 'Ver', ubication %></td>
    <td class="wmi"><%= link_to 'Editar', edit_ubication_path(ubication) %></td>
    <td class="wmi"><%= link_to 'Eliminar', ubication, :confirm => 'Está seguro?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Nueva ubicación', new_ubication_path %>
</div>
<%= render :partial => "admin/section_foot" %>