<h1>New wmi_method</h1>

<% form_for(@wmi_method) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :wmi_class_id %><br />
    <%= f.text_field :wmi_class_id %>
  </p>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :alias %><br />
    <%= f.text_field :alias %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_field :description %>
  </p>
  <p>
    <%= f.label :method %><br />
    <%= f.text_field :method %>
  </p>
  <p>
    <%= f.label :parameter %><br />
    <%= f.text_field :parameter %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', wmi_methods_path %>